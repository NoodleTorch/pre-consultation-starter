{
  "title": "Pre-Consultation (Ophthalmology)",
  "start": "consent",
  "questions": {
    "consent": {
      "id": "consent",
      "type": "single",
      "label": "Before we begin — please read and acknowledge",
      "help": "Do you consent to completing a short pre‑consultation questionnaire? Your answers will be reviewed by the clinic team. You can stop anytime. Your answers are kept on this device until you choose to submit.",
      "input": { "style": "big" },
      "options": [
        { "value": "ok", "label": "I agree — continue" }
      ],
      "next": "safety_screen"
    },

    "safety_screen": {
      "id": "safety_screen",
      "type": "single",
      "required": true,
      "label": "In the last 7 days, have you had any of the following?",
      "help": "• Sudden loss of vision\n• Severe eye pain/redness",
      "input": { "style": "big" },
      "options": [
        { "value": "yes", "label": "Yes" },
        { "value": "no", "label": "No" },
        { "value": "unsure", "label": "I’m not sure" }
      ],
      "next": { "when": { "yes": "urgent_guidance", "unsure": "urgent_guidance" }, "default": "reason_for_visit" }
    },

    "urgent_guidance": {
      "id": "urgent_guidance",
      "type": "single",
      "required": false,
      "label": "Important — please read",
      "help": "These symptoms can be urgent. Please attend your nearest Emergency Department or see an optometrist today. You can still complete this form for your upcoming visit.",
      "input": { "style": "big" },
      "options": [ { "value": "ok", "label": "OK, continue" } ],
      "next": "reason_for_visit"
    },

    "reason_for_visit": {
      "id": "reason_for_visit",
      "type": "single",
      "required": true,
      "label": "What is the main reason for your appointment?",
      "input": { "style": "big" },
      "options": [
        { "value": "glaucoma", "label": "Glaucoma / eye pressure check" },
        { "value": "cataract", "label": "Cataract assessment or symptoms" },
        { "value": "other_issue", "label": "Other eye issue (type briefly)" },
        { "value": "general_check", "label": "General eye check / second opinion" },
        { "value": "unsure_symptoms", "label": "New or worsening symptoms (not sure what)" }
      ],
      "next": "age"
    },

    "age": {
      "id": "age",
      "type": "slider",
      "required": true,
      "label": "Age",
      "help": "Slide or tap to set your age. You can also type a number.",
      "input": { "min": 18, "max": 100, "step": 1, "showValue": true },
      "next": "sex_at_birth"
    },

    "sex_at_birth": {
      "id": "sex_at_birth",
      "type": "single",
      "required": true,
      "label": "Gender",
      "input": { "style": "big" },
      "options": [
        { "value": "male", "label": "Male" },
        { "value": "female", "label": "Female" },
        { "value": "intersex_other", "label": "Intersex / another option" },
        { "value": "prefer_not", "label": "Prefer not to say" }
      ],
      "next": "ethnicity"
    },

    "ethnicity": {
      "id": "ethnicity",
      "type": "single",
      "required": true,
      "label": "Ancestry (for risk assessment)",
      "input": { "style": "big" },
      "options": [
        { "value": "caucasian", "label": "Caucasian" },
        { "value": "african", "label": "African" },
        { "value": "asian", "label": "Asian" },
        { "value": "indigenous_aus", "label": "Indigenous Australian" },
        { "value": "other", "label": "Other" },
        { "value": "prefer_not_say", "label": "Prefer not to say" }
      ],
      "next": { "when": { "other": "ethnicity_other" }, "default": "affected_eye" }
    },

    "ethnicity_other": {
      "id": "ethnicity_other",
      "type": "text",
      "required": true,
      "label": "Please specify your ancestry",
      "input": { "placeholder": "e.g., Middle Eastern, Pacific Islander…" },
      "next": "affected_eye"
    },

    "affected_eye": {
      "id": "affected_eye",
      "type": "single",
      "required": true,
      "label": "Which eye bothers you the most?",
      "input": { "style": "big" },
      "options": [
        { "value": "left", "label": "Left" },
        { "value": "right", "label": "Right" },
        { "value": "both", "label": "Both" },
        { "value": "not_sure", "label": "Not sure" }
      ],
      "next": "driving"
    },

    "driving": {
      "id": "driving",
      "type": "single",
      "required": true,
      "label": "Do you currently drive?",
      "input": { "style": "big" },
      "options": [
        { "value": "regular", "label": "Yes, regularly" },
        { "value": "occasional", "label": "Occasionally" },
        { "value": "not_driving", "label": "Not driving" }
      ],
      "next": {
        "byAnswerOf": "reason_for_visit",
        "map": {
          "glaucoma": "A3_symptoms",
          "cataract": "B3_symptoms",
          "other_issue": "D_main_symptom",
          "general_check": "D_main_symptom",
          "unsure_symptoms": "D_main_symptom"
        },
        "default": "D_main_symptom"
      }
    },

    "A3_symptoms": {
      "id": "A3_symptoms",
      "type": "multi",
      "label": "Glaucoma symptoms & function (tap all that apply)",
      "options": [
        { "value": "peripheral_change", "label": "Change to side (peripheral) vision" },
        { "value": "pressure", "label": "Eye discomfort/pressure now" },
        { "value": "night_vision", "label": "Night vision difficulties" },
        { "value": "photophobia", "label": "Light sensitivity or headaches" },
        { "value": "reading", "label": "Reading" },
        { "value": "driving", "label": "Driving" },
        { "value": "stairs", "label": "Stairs" },
        { "value": "tv", "label": "TV/subtitles" },
        { "value": "hobbies", "label": "Hobbies" },
        { "value": "none", "label": "None" }
      ],
      "next": "A4_type"
    },

    "A4_type": {
      "id": "A4_type",
      "type": "single",
      "label": "Has a clinician told you the type of glaucoma?",
      "input": { "style": "big" },
      "options": [
        { "value": "poag", "label": "Open-angle (POAG)" },
        { "value": "angle_closure", "label": "Angle-closure" },
        { "value": "secondary", "label": "Secondary" },
        { "value": "none", "label": "No glaucoma diagnosis" },
        { "value": "unsure", "label": "Not sure" }
      ],
      "next": "A4_last_check"
    },

    "A4_last_check": {
      "id": "A4_last_check",
      "type": "single",
      "label": "When was your last glaucoma check?",
      "input": { "style": "big" },
      "options": [
        { "value": "lt3m", "label": "Under 3 months ago" },
        { "value": "3to12m", "label": "3–12 months ago" },
        { "value": "gt12m", "label": "More than 12 months ago" },
        { "value": "unsure", "label": "Not sure" }
      ],
      "next": "A4_iop_know"
    },

    "A4_iop_know": {
      "id": "A4_iop_know",
      "type": "single",
      "label": "Do you know your usual eye pressure (IOP) or target?",
      "input": { "style": "big" },
      "options": [
        { "value": "yes", "label": "Yes" },
        { "value": "no", "label": "No" },
        { "value": "unsure", "label": "Not sure" }
      ],
      "next": { "when": { "yes": "A4_iop_value" }, "default": "A4_tests" }
    },

    "A4_iop_value": {
      "id": "A4_iop_value",
      "type": "text",
      "label": "Enter IOP/target (if known)",
      "input": { "placeholder": "e.g., Right 14, Left 16" },
      "next": "A4_tests"
    },

    "A4_tests": {
      "id": "A4_tests",
      "type": "multi",
      "label": "Which tests have you had? (tap all)",
      "options": [
        { "value": "vf", "label": "Visual field (button-press test)" },
        { "value": "oct", "label": "OCT of the optic nerve" },
        { "value": "cct", "label": "Corneal thickness" },
        { "value": "gonio", "label": "Angle check (gonioscopy)" },
        { "value": "unsure", "label": "Not sure" }
      ],
      "next": "A4_fhx"
    },

    "A4_fhx": {
      "id": "A4_fhx",
      "type": "single",
      "label": "Glaucoma in parent/sibling/child?",
      "input": { "style": "big" },
      "options": [
        { "value": "yes", "label": "Yes" },
        { "value": "no", "label": "No" },
        { "value": "unsure", "label": "Not sure" }
      ],
      "next": "A5_drops"
    },

    "A5_drops": {
      "id": "A5_drops",
      "type": "multi",
      "label": "Current glaucoma drops (tap all)",
      "help": "Include brand names you recognise; pictures to be added later.",
      "options": [
        { "value": "latanoprost", "label": "Xalatan/Monopost (latanoprost)" },
        { "value": "bimatoprost", "label": "Lumigan/Ganfort (bimatoprost ± timolol)" },
        { "value": "travoprost", "label": "Travatan/Duotrav (travoprost ± timolol)" },
        { "value": "timolol", "label": "Timoptol/Timolol XE (timolol)" },
        { "value": "betaxolol", "label": "Betoptic (betaxolol)" },
        { "value": "brimonidine", "label": "Alphagan/Alphagan P (brimonidine)" },
        { "value": "brinzolamide", "label": "Azopt/Azarga (brinzolamide ± timolol)" },
        { "value": "dorzolamide", "label": "Trusopt/Cosopt (dorzolamide ± timolol)" },
        { "value": "simbrinza", "label": "Simbrinza (brinzolamide + brimonidine)" },
        { "value": "rho_inhib", "label": "Rocklatan/Rhokiinsa class" },
        { "value": "other", "label": "Other" },
        { "value": "not_sure", "label": "Not sure" },
        { "value": "none", "label": "None" }
      ],
      "next": "A5_adherence"
    },

    "A5_adherence": {
      "id": "A5_adherence",
      "type": "single",
      "label": "In a usual week, how often do you miss doses?",
      "input": { "style": "big" },
      "options": [
        { "value": "never", "label": "Never" },
        { "value": "1to2", "label": "1–2 times" },
        { "value": "3to4", "label": "3–4 times" },
        { "value": "5plus", "label": "≥ 5 times" },
        { "value": "not_on_drops", "label": "Not using drops" }
      ],
      "next": "A5_side_effects"
    },

    "A5_side_effects": {
      "id": "A5_side_effects",
      "type": "multi",
      "label": "Side effects (tap all)",
      "options": [
        { "value": "redness", "label": "Redness" },
        { "value": "stinging", "label": "Stinging" },
        { "value": "wheeze", "label": "Wheeze/short breath" },
        { "value": "slow_pulse", "label": "Slow pulse/dizziness" },
        { "value": "lashes", "label": "Eyelash/skin darkening" },
        { "value": "other", "label": "Other" },
        { "value": "none", "label": "None" }
      ],
      "next": "A5_prev_procedures"
    },

    "A5_prev_procedures": {
      "id": "A5_prev_procedures",
      "type": "multi",
      "label": "Previous glaucoma procedures (tap all)",
      "help": "Memory joggers shown on tiles; please add details next if known.",
      "options": [
        { "value": "slt", "label": "SLT — laser to help the eye’s drain work better" },
        { "value": "lpi", "label": "LPI — tiny laser hole in the iris" },
        { "value": "trab", "label": "Trabeculectomy — small flap to drain fluid" },
        { "value": "tube_migs", "label": "Tube/MIGS — tiny stent or tube" },
        { "value": "cyclo", "label": "Cyclo-laser — reduce fluid production" },
        { "value": "none", "label": "None" },
        { "value": "unsure", "label": "Not sure" }
      ],
      "next": "A5_procedure_details"
    },

    "A5_procedure_details": {
      "id": "A5_procedure_details",
      "type": "text",
      "label": "Procedure details (eye/year if known)",
      "input": { "placeholder": "e.g., SLT right 2022; Trab left 2018" },
      "next": "E1_history"
    },

    "B3_symptoms": {
      "id": "B3_symptoms",
      "type": "multi",
      "label": "Cataract symptoms (tap all that apply)",
      "options": [
        { "value": "blurry", "label": "Blurry or cloudy vision" },
        { "value": "glare", "label": "Glare or halos" },
        { "value": "night_driving", "label": "Trouble with night driving" },
        { "value": "faded_colours", "label": "Colours look faded" },
        { "value": "mono_double", "label": "Double vision in one eye" },
        { "value": "other", "label": "Other" },
        { "value": "none", "label": "None" }
      ],
      "next": "B3_duration"
    },

    "B3_duration": {
      "id": "B3_duration",
      "type": "single",
      "label": "How long have these symptoms been going on?",
      "input": { "style": "big" },
      "options": [
        { "value": "lt1m", "label": "Under 1 month" },
        { "value": "1to6m", "label": "1–6 months" },
        { "value": "6to24m", "label": "6–24 months" },
        { "value": "gt2y", "label": "More than 2 years" },
        { "value": "unsure", "label": "Not sure" }
      ],
      "next": "B3_tasks"
    },

    "B3_tasks": {
      "id": "B3_tasks",
      "type": "multi",
      "label": "Which everyday tasks are hardest now? (tap all)",
      "options": [
        { "value": "reading", "label": "Reading" },
        { "value": "night_driving", "label": "Night driving" },
        { "value": "computer", "label": "Computer" },
        { "value": "tv", "label": "TV" },
        { "value": "hobbies", "label": "Hobbies" },
        { "value": "stairs", "label": "Stairs / avoiding falls" },
        { "value": "none", "label": "None" }
      ],
      "next": "B3_past_procedures"
    },

    "B3_past_procedures": {
      "id": "B3_past_procedures",
      "type": "multi",
      "label": "Past eye procedures",
      "options": [
        { "value": "cataract_sx", "label": "Cataract surgery (eye/year)" },
        { "value": "yag", "label": "YAG laser (eye/year)" },
        { "value": "none", "label": "None" },
        { "value": "unsure", "label": "Not sure" }
      ],
      "next": "B3_proc_details"
    },

    "B3_proc_details": {
      "id": "B3_proc_details",
      "type": "text",
      "label": "If selected above, add eye/year details",
      "input": { "placeholder": "e.g., Right 2021; Left YAG 2023" },
      "next": "B4_discuss"
    },

    "B4_discuss": {
      "id": "B4_discuss",
      "type": "single",
      "label": "Would you like to talk about cataract surgery with the doctor?",
      "input": { "style": "big" },
      "options": [
        { "value": "yes", "label": "Yes" },
        { "value": "unsure", "label": "Not sure" },
        { "value": "no", "label": "No" }
      ],
      "next": { "when": { "yes": "B4_info", "unsure": "B4_info" }, "default": "B4_pref_goal" }
    },

    "B4_info": {
      "id": "B4_info",
      "type": "text",
      "required": false,
      "label": "Lens options (quick neutral overview)",
      "input": { "textarea": true, "readonly": true },
      "preset": "Monofocal: clearest at one distance; reading glasses are common after surgery.\nToric (astigmatism-correcting): may reduce glasses at the chosen distance.\nMultifocal/EDOF: may reduce glasses for more distances but can cause halos/glare and aren’t for everyone.\nNo lens is perfect. Your surgeon will advise what suits your eyes and lifestyle.",
      "next": "B4_pref_goal"
    },

    "B4_pref_goal": {
      "id": "B4_pref_goal",
      "type": "single",
      "label": "If suitable, which is more important to you?",
      "input": { "style": "big" },
      "options": [
        { "value": "distance", "label": "Best distance vision" },
        { "value": "near", "label": "Best near vision" },
        { "value": "balanced", "label": "Balanced" },
        { "value": "unsure", "label": "Not sure" }
      ],
      "next": "B4_night_importance"
    },

    "B4_night_importance": {
      "id": "B4_night_importance",
      "type": "single",
      "label": "How important is night driving to you?",
      "input": { "style": "big" },
      "options": [
        { "value": "not", "label": "Not" },
        { "value": "somewhat", "label": "Somewhat" },
        { "value": "very", "label": "Very" }
      ],
      "next": "B4_reading_glasses"
    },

    "B4_reading_glasses": {
      "id": "B4_reading_glasses",
      "type": "single",
      "label": "Are you okay using reading glasses after surgery?",
      "input": { "style": "big" },
      "options": [
        { "value": "yes", "label": "Yes" },
        { "value": "avoid", "label": "I’d prefer to avoid them" },
        { "value": "unsure", "label": "Not sure" }
      ],
      "next": "B4_prior_refractive"
    },

    "B4_prior_refractive": {
      "id": "B4_prior_refractive",
      "type": "single",
      "label": "Prior refractive laser (LASIK/PRK/SMILE)?",
      "input": { "style": "big" },
      "options": [
        { "value": "yes", "label": "Yes" },
        { "value": "no", "label": "No" },
        { "value": "unsure", "label": "Not sure" }
      ],
      "next": { "when": { "yes": "B4_prior_refractive_type" }, "default": "B4_astigmatism" }
    },

    "B4_prior_refractive_type": {
      "id": "B4_prior_refractive_type",
      "type": "single",
      "label": "Which type of laser vision correction?",
      "input": { "style": "big" },
      "options": [
        { "value": "lasik", "label": "LASIK" },
        { "value": "prk", "label": "PRK" },
        { "value": "smile", "label": "SMILE" },
        { "value": "not_sure", "label": "Not sure" }
      ],
      "next": { "when": { "not_sure": "B4_astigmatism" }, "default": "B4_prior_refractive_when" }
    },

    "B4_prior_refractive_when": {
      "id": "B4_prior_refractive_when",
      "type": "single",
      "label": "When was it done?",
      "input": { "style": "big" },
      "options": [
        { "value": "lt1y", "label": "Within the last year" },
        { "value": "1to3y", "label": "1–3 years ago" },
        { "value": "3to5y", "label": "3–5 years ago" },
        { "value": "gt5y", "label": "Over 5 years ago" },
        { "value": "unsure", "label": "Not sure" }
      ],
      "next": "B4_astigmatism"
    },

    "B4_astigmatism": {
      "id": "B4_astigmatism",
      "type": "single",
      "label": "Has anyone told you that you have astigmatism?",
      "input": { "style": "big" },
      "options": [
        { "value": "yes", "label": "Yes" },
        { "value": "no", "label": "No" },
        { "value": "unsure", "label": "Not sure" }
      ],
      "next": {
        "byAnswerOf": "sex_at_birth",
        "map": { "male": "B5_prostate_meds" },
        "default": "B5_blood_thinners"
      }
    },

    "B5_prostate_meds": {
      "id": "B5_prostate_meds",
      "type": "single",
      "label": "Do you take any medicines for prostate or urinary symptoms?",
      "help": "Examples: tamsulosin (Flomax), alfuzosin, terazosin, doxazosin, silodosin, Duodart.",
      "input": { "style": "big" },
      "options": [
        { "value": "yes", "label": "Yes" },
        { "value": "no", "label": "No" },
        { "value": "unsure", "label": "I’m not sure" }
      ],
      "next": "B5_blood_thinners"
    },

    "B5_blood_thinners": {
      "id": "B5_blood_thinners",
      "type": "single",
      "label": "Do you take any blood-thinning medicines?",
      "help": "Examples: warfarin, apixaban (Eliquis), rivaroxaban (Xarelto), clopidogrel (Plavix).",
      "input": { "style": "big" },
      "options": [
        { "value": "yes", "label": "Yes" },
        { "value": "no", "label": "No" },
        { "value": "unsure", "label": "I’m not sure" }
      ],
      "next": { "when": { "yes": "B5_blood_thinners_which" }, "default": "B5_lie_flat" }
    },

    "B5_blood_thinners_which": {
      "id": "B5_blood_thinners_which",
      "type": "multi",
      "label": "Which medicine? (tap all that apply)",
      "options": [
        { "value": "warfarin", "label": "Warfarin" },
        { "value": "apixaban", "label": "Apixaban (Eliquis)" },
        { "value": "rivaroxaban", "label": "Rivaroxaban (Xarelto)" },
        { "value": "dabigatran", "label": "Dabigatran (Pradaxa)" },
        { "value": "clopidogrel", "label": "Clopidogrel (Plavix)" },
        { "value": "aspirin", "label": "Aspirin" },
        { "value": "other", "label": "Other" },
        { "value": "unsure", "label": "Not sure" }
      ],
      "next": { "when": { "other": "B5_blood_thinners_other" }, "default": "B5_lie_flat" }
    },

    "B5_blood_thinners_other": {
      "id": "B5_blood_thinners_other",
      "type": "text",
      "label": "Please type the name (optional)",
      "input": { "placeholder": "e.g., edoxaban" },
      "next": "B5_lie_flat"
    },

    "B5_lie_flat": {
      "id": "B5_lie_flat",
      "type": "single",
      "label": "Can you lie flat comfortably for 20–30 minutes?",
      "input": { "style": "big" },
      "options": [
        { "value": "yes", "label": "Yes" },
        { "value": "no", "label": "No" },
        { "value": "unsure", "label": "Not sure" }
      ],
      "next": { "when": { "no": "B5_lie_flat_followup", "unsure": "B5_lie_flat_followup" }, "default": "B5_ocular_dom" }
    },

    "B5_lie_flat_followup": {
      "id": "B5_lie_flat_followup",
      "type": "single",
      "label": "Do neck or back issues prevent you from lying flat?",
      "input": { "style": "big" },
      "options": [
        { "value": "yes", "label": "Yes" },
        { "value": "no", "label": "No" },
        { "value": "unsure", "label": "Not sure" }
      ],
      "next": "B5_ocular_dom"
    },

    "B5_ocular_dom": {
      "id": "B5_ocular_dom",
      "type": "ocular_dominance",
      "label": "Ocular dominance (optional quick check)",
      "help": "Use the finger-circle method or try the webcam helper.",
      "next": "E1_history"
    },

    "C_main_concern": {
      "id": "C_main_concern",
      "type": "single",
      "label": "What best describes your main concern?",
      "input": { "style": "big" },
      "options": [
        { "value": "blur", "label": "Vision blur" },
